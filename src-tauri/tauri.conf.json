{
  "productName": "Il Bello Del Fare Info",
  "version": "1.0.0",
  "identifier": "org.thundernetwork.il-bello-del-fare-info",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../web/"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Il Bello Del Fare Info",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
